      "question_type": "rating",
      "question_text": "How easy was the onboarding process?",
      "description": "Rate your overall experience with the platform setup and initial training",
      "required": true,
      "options": {
        "min": 1,
        "max": 10,
        "labels": {
          "1": "Very Difficult",
          "5": "Neutral",
          "10": "Very Easy"
        }
      },
      "category": "usability"
    },
    {
      "question_id": "onboarding_002",
      "question_type": "boolean",
      "question_text": "Were the training materials helpful?",
      "description": "Did the provided documentation, videos, and guides help you understand the platform?",
      "required": true,
      "options": {
        "true_label": "Yes, very helpful",
        "false_label": "No, not helpful"
      },
      "category": "training"
    },
    {
      "question_id": "onboarding_003",
      "question_type": "rating",
      "question_text": "How confident do you feel using the platform?",
      "description": "Rate your confidence level in using the platform's core features",
      "required": true,
      "options": {
        "min": 1,
        "max": 10,
        "labels": {
          "1": "Not Confident",
          "5": "Somewhat Confident",
          "10": "Very Confident"
        }
      },
      "category": "confidence"
    },
    {
      "question_id": "onboarding_004",
      "question_type": "multiple_choice",
      "question_text": "Which onboarding materials were most helpful?",
      "description": "Select all that apply",
      "required": false,
      "options": {
        "choices": [
          "Video tutorials",
          "Written documentation",
          "Interactive walkthrough",
          "Sample data and examples",
          "Live training session",
          "FAQ section",
          "Support team assistance",
          "None of the above"
        ],
        "allow_multiple": true
      },
      "category": "training"
    },
    {
      "question_id": "onboarding_005",
      "question_type": "text",
      "question_text": "What challenges did you encounter during onboarding?",
      "description": "Please describe any difficulties or issues you faced",
      "required": false,
      "options": {
        "max_length": 500,
        "placeholder": "Describe any challenges you encountered..."
      },
      "category": "challenges"
    },
    {
      "question_id": "onboarding_006",
      "question_type": "text",
      "question_text": "What would improve the onboarding experience?",
      "description": "Please share your suggestions for making the onboarding process better",
      "required": false,
      "options": {
        "max_length": 500,
        "placeholder": "Share your suggestions for improvement..."
      },
      "category": "improvements"
