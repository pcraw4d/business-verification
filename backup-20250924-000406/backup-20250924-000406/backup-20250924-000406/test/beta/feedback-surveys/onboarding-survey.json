    {
      "question_id": "onboarding_007",
      "question_type": "rating",
      "question_text": "How satisfied are you with the support provided during onboarding?",
      "description": "Rate your satisfaction with the support team and resources",
      "required": true,
      "options": {
        "min": 1,
        "max": 10,
        "labels": {
          "1": "Very Dissatisfied",
          "5": "Neutral",
          "10": "Very Satisfied"
        }
      },
      "category": "support"
    },
    {
      "question_id": "onboarding_008",
      "question_type": "multiple_choice",
      "question_text": "How long did it take you to complete the onboarding process?",
      "description": "From initial access to feeling comfortable using the platform",
      "required": true,
      "options": {
        "choices": [
          "Less than 1 hour",
          "1-2 hours",
          "2-4 hours",
          "4-8 hours",
          "More than 8 hours"
        ],
        "allow_multiple": false
      },
      "category": "time"
    },
    {
      "question_id": "onboarding_009",
      "question_type": "boolean",
      "question_text": "Would you recommend the onboarding process to others?",
      "description": "Based on your experience, would you recommend this onboarding process to colleagues?",
      "required": true,
      "options": {
        "true_label": "Yes, I would recommend it",
        "false_label": "No, I would not recommend it"
      },
      "category": "recommendation"
    },
    {
      "question_id": "onboarding_010",
      "question_type": "text",
      "question_text": "Additional comments about your onboarding experience",
      "description": "Any other feedback or comments you'd like to share",
      "required": false,
      "options": {
        "max_length": 1000,
        "placeholder": "Share any additional thoughts or feedback..."
      },
      "category": "general"
    }
