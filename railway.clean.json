{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.clean"
  },
  "deploy": {
    "startCommand": "./railway-server",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "numReplicas": 1
  }
}
