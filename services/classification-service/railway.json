{
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "startCommand": "./classification-service",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "numReplicas": 1,
    "healthcheckPath": "/health",
    "healthcheckTimeout": 10,
    "healthcheckInterval": 30
  }
}
