{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "cmd/frontend-service/Dockerfile",
    "dockerContext": "."
  },
  "deploy": {
    "startCommand": "./frontend-service",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "healthcheckInterval": 30,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}

